<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>佣金发放确认</title>
<link href="../../css/app.css" type="text/css" rel="stylesheet" />
<link href="../../css/zx-all.css" rel="stylesheet" type="text/css" />
<script src="../../js/zx-all.js" type="text/javascript"></script>

<script type="text/javascript">	
	$(function() {
		var data_time = $.query.get("data_time");
		var data_time_month = $.query.get("data_time_month");
		data_time="请确认"+data_time+"佣金已发放完毕，确认后将不再显示"+data_time_month+"佣金情况";
		$("#data_time").text(data_time);
	});
 	//关闭页面
    function closePage(){
    	window.parent.dialog.hide();
    }
 	//修改单据
 	function update(){
 		window.parent.update_date();
 		
 	}
</script>
</head>
<body>
	<div>
		<div class="pop-formDiv clearfix" style="margin: 5px;">
			<div class="center-content clearboth" style="min-width: 200px;">
				<div style="margin-top: 40px" align="center"><span style="font-family: 华文楷体;color: red;font-size: 18.0pt" id="data_time"></span></div>
			</div>
		</div>
	</div>

	<!-- 提交功能按钮区 -->
	<div class="pop-footer5 clearboth" style="bottom: 1px;" id="tb_btn">
		<input id="tjbtn" onclick="update();" class="btn-saveT"
			onmouseover="this.className='btn-saveT-over'"
			onmouseout="this.className='btn-saveT'"
			onmousedown="this.className='btn-saveT-down'" type="button"
			style="margin-right: 7px;" /> 
		<input id="cancelBtnId" class="btn-cancel" onmouseover="this.className='btn-cancel-over'"
			onmouseout="this.className='btn-cancel'"
			onmousedown="this.className='btn-cancel-down'" type="button"
			onclick="closePage();" />
	</div>
</body>
</html>
