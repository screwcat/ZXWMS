//初始化借据中的还款日期
function setPlan_borrow_date(jsonForSys){
	   jsonForSys.plan_borrow_date="";//   现金借据中，还款日期删除默认值。 支票借据中，还款日期删除默认值。
	 //jsonForSys.plan_borrow_date1=jsonForSys.plan_borrow_date;
}
/****************页面上的金额**********************/
//<借款合同>借款本金_小写=//面签确定金额 / (1-(月利率+居间服务利率)*期数) ，进位取整   
function setPrincipal_lower(jsonForSys){
	 jsonForSys.principal_lower=Math.ceil(parseFloat(jsonForSys.principal_lower)*((parseFloat(jsonForSys.service_cost_month)+parseFloat(jsonForSys.borrow_interest))/100)*parseFloat(jsonForSys.borrow_deadline)+parseFloat(jsonForSys.principal_lower));
	 //jsonForSys.principal_lower=Math.ceil(parseFloat(jsonForSys.principal_lower)/(1-(parseFloat(jsonForSys.service_cost_month)+parseFloat(jsonForSys.borrow_interest))/100*parseFloat(jsonForSys.borrow_deadline)));
}
//初始化每月还款金额数据来源：该数值 = 借款合同内的借款本金 div 借款合同内的借款期限，进位取整
function setRefund_limit_month(jsonForSys){
	 jsonForSys.refund_limit_month=Math.ceil(parseFloat(jsonForSys.principal_lower)/parseFloat(jsonForSys.borrow_deadline));
}
//初始化违约金 借款合同内的借款本金 * 每日违约金利率。系统提供默认值，现阶段可手工修改。其中每日违约金利率可后台配置，房贷=0.1%。
function setLiquidated_damages(jsonForSys){
	 jsonForSys.liquidated_damages=Math.ceil(parseFloat(jsonForSys.principal_lower)*parseFloat(jsonForSys.weiyuejin)*0.01);
}
//初始化居间服务费    居间服务费月利率 * 贷款期数 。
function setJujian_service_cost(jsonForSys){
	 jsonForSys.jujian_service_cost=parseFloat(jsonForSys.service_cost_month*1000000)*parseFloat(jsonForSys.borrow_deadline)/1000000;
}
//小写转大写
function setCaps(jsonForSys){
	 //设置借款金额大小写转换
	jsonForSys.principal_caps=convertCurrency(jsonForSys.principal_lower);
	 //设置借据中的转换
	jsonForSys.principal_caps1=convertCurrency(jsonForSys.principal_lower);
	 //设置收据中的转换
	jsonForSys.principal_caps2=convertCurrency(jsonForSys.principal_lower);
	 //设置借据中的借款金额
	jsonForSys.principal_caps3=convertCurrency(jsonForSys.principal_lower);
	 //设置借据中的支票金额大写
	jsonForSys.principal_caps4=convertCurrency(jsonForSys.principal_lower);
	 //设置借据中的利息大小写转换
	jsonForSys.borrow_interest_caps="百分之"+convertCurrency(jsonForSys.borrow_interest);
}
