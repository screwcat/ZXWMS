				"protocol_type": protocol_type,
				"wms_cre_credit_head_id": wms_cre_credit_head_id
			},
			function(jsonForSys) {
				//记录违约金利率
				weiyuejin=jsonForSys.weiyuejin;
				/*jsonForSys.principal_lower=392400;
				jsonForSys.borrow_deadline=12;
				jsonForSys.service_cost_month=1.35;*/
				//初始化债权人姓名
				setCreditor_name(jsonForSys);
				//初始化债务人姓名
				setDebtor_name(jsonForSys);
				//初始化债务人身份证号
				setDebtor_identity_id(jsonForSys);
				//初始化借款期数
				setBorrow_deadline(jsonForSys);
				//初始化签订日期
				setProtocol_date(jsonForSys);
				//初始化月利息
				setBorrow_interest(jsonForSys);
				//初始化还款日
				setRefund_day(jsonForSys);
				if(jsonForSys.com_debtor_name!=null&&jsonForSys.com_debtor_name!=""){	
					//初始化共同债务人姓名
					setCom_debtor_name(jsonForSys);
				}
				if(jsonForSys.com_debtor_identity_id!=null&&jsonForSys.com_debtor_identity_id!=""){	
					//初始化共同债务人身份证
					setCom_debtor_identity_id(jsonForSys);
				}
				//初始化合同首页债务人姓名  =债务人姓名+共同债务人姓名
				setDebtor_name1(jsonForSys);
				//初始化借款人通讯地址
				//setDebtor_address(jsonForSys);
				//初始化借款人联系电话（居间服务协议） 
				//setDebtor_tel(jsonForSys);
				//初始化合同编号年
				setProtocol_id_year(jsonForSys);
				//初始化合同编号号
				setProtocol_id_num(jsonForSys);
				//初始化借据中的还款日期--现金借据
				setPlan_borrow_date(jsonForSys);
				//********************金额计算***************************//
				//<借款合同>借款本金_小写=//面签确定金额 / (1-(月利率+居间服务利率)*期数) ，进位取整   
				setPrincipal_lower(jsonForSys);
				//初始化借款金额小写(再重新计算完借款金额后赋值)
				setPrincipal_lowerAfter(jsonForSys);
				//初始化每月还款金额该数值 = 借款合同内的借款本金 * （借款月利率+居间服务利率），进位取整   。borrow_interest
				setRefund_limit_month(jsonForSys);
				//初始化违约金 借款合同内的借款本金 * 每日违约金利率。系统提供默认值，现阶段可手工修改。其中每日违约金利率可后台配置，房贷=0.1%。
				setLiquidated_damages(jsonForSys);
				//初始化居间服务费    居间服务费月利率 * 贷款期数 。
				setJujian_service_cost(jsonForSys);
				 //小写转大写
				setCaps(jsonForSys);
				//cash_transfer_lower--支票借据  现金/转账
				setcash_transfer_lower(jsonForSys);
				//设置居间服务协议
				setJuJianFuWu(jsonForSys);
				//********************金额计算***************************//
				//初始化赋值
				globalUtil.setFormVal("jkht", jsonForSys);	
				//设置只读字段
				//set();
		});
	});
	
//*********************************************初始化数据******************************************************/
//cash_transfer_lower--支票借据  现金/装张
function setcash_transfer_lower(jsonForSys){
	jsonForSys.cash_transfer_lower="现金/转账";
}
////居间服务协议中，“乙方通讯地址”/“收件人”/“联系电话”3个字段，当是共贷时，这3个字段数据共贷人信息，而非主贷人信息。
////当是单独贷款时，来源唯一贷款人信息。
//设置居间服务协议
function setJuJianFuWu(jsonForSys){
	if(jsonForSys.com_debtor_name!=null){
		jsonForSys.addressee_name = jsonForSys.com_debtor_name;//收件人
		jsonForSys.borrower_address = jsonForSys.com_debtor_address;//乙方通讯地址
		jsonForSys.addressee_tel = jsonForSys.com_debtor_tel;//联系电话
	}else{
		jsonForSys.addressee_name = jsonForSys.debtor_name;//收件人
		jsonForSys.borrower_address = jsonForSys.debtor_address;//乙方通讯地址
		jsonForSys.addressee_tel = jsonForSys.debtor_tel;//联系电话
	}
}
//初始化债权人姓名
function setCreditor_name(jsonForSys){
	 jsonForSys.creditor_name1=jsonForSys.creditor_name;
	 jsonForSys.creditor_name2=jsonForSys.creditor_name;
	 jsonForSys.creditor_name3=jsonForSys.creditor_name;
	 jsonForSys.creditor_name4=jsonForSys.creditor_name;
}
//初始化债务人
function setDebtor_name(jsonForSys){
	 jsonForSys.debtor_name2=jsonForSys.debtor_name;//借据1中的
