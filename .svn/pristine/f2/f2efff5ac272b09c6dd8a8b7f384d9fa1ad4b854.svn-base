	 jsonForSys.debtor_name4=jsonForSys.debtor_name;//借据2中的
	 jsonForSys.debtor_name5=jsonForSys.debtor_name;//借据2中的
	 jsonForSys.borrower_name=jsonForSys.debtor_name;//设置居间服务中的借款人姓名
	 jsonForSys.person_name1=jsonForSys.debtor_name;//设置抵押人姓名
	 //jsonForSys.addressee_name=jsonForSys.debtor_name;//设置收件人姓名（居间服务协议）
}
