$(function()  {
	 $(".borrow_page").hide(); 
	wms_cre_credit_head_id  =  $.query.get('wms_cre_credit_head_id');
	//初始化数据
	$.getJSON(globalUtil.getTimestampUrl('/loanappro/wmssyspropertylistforhouse.do'),////初始化数据
