	//弹出错误提示框
	 function errorMsg(msg,ele){
	 	globalUtil.addMask('1');
	 	var obj = $.ligerDialog.error(msg, null, null);
	 	obj.options.onHiddenOrClose = function() {
	 		$(ele).val('');
	 		$(ele).focus();
	 		globalUtil.delMask('1');
	     };
	 }
	 var sticker_price;
	 var total_tax_amount;
	 //计算挂牌价
	 function calculateStickerPrice(ele){
		 stickerPriceCheck(ele);
		 var area = $("#house_building_area1").val();
		 var price = $("#sticker_unit_price").val();
		 if(area!=''){
			 $("#transaction_fee_is").val((area*6).toFixed(0));
			 if(price!=''){
				 sticker_price = price*area;
				 $("#sticker_price").val(sticker_price.toFixed(0));
				 $("#tax_amount_is").val((sticker_price*0.0565).toFixed(0));
				 $("#tax_amount_no").val((sticker_price*0.0565).toFixed(0));
				 $("#personal_income_tax_is").val((sticker_price*0.01).toFixed(0));
				 $("#personal_income_tax_no").val((sticker_price*0.02).toFixed(0));
				 $("#deed_tax_is_one").val((sticker_price*0.015).toFixed(0));
				 $("#deed_tax_is_two").val((sticker_price*0.03).toFixed(0));
				 $("#deed_tax_no").val((sticker_price*0.04).toFixed(0));
				 $("#land_appreciation_tax_no").val((sticker_price*0.05).toFixed(0));
				 $("#transaction_fee_no").val((sticker_price*0.01).toFixed(0));
				 if($("#total_tax_amount").val()!=''&&sticker_price!=''){
					 $("#transaction_price").val((sticker_price*0.95-total_tax_amount).toFixed(0));
					 $("#transaction_price1").val((sticker_price*0.95-total_tax_amount).toFixed(0));
					 $("#six_amount").val(((sticker_price*0.95-total_tax_amount)*0.6).toFixed(0));
					 $("#nine_amount").val(((sticker_price*0.95-total_tax_amount)*0.9).toFixed(0));
		 			 $("#total_amount").val((sticker_price*0.95-total_tax_amount).toFixed(0));
				 }else{
					 $("#transaction_price").val('');
					 $("#transaction_price1").val('');
					 $("#six_amount").val('');
					 $("#nine_amount").val('');
					 $("#total_amount").val('');
				 }
			 }else{
				 $("#sticker_price").val('');
				 sticker_price = '';
			 }
		 }else{
			 $("#sticker_price").val('');
			 sticker_price = '';
		 };
	 }
	 //清空与挂牌单价和面积相关数据
	 function toStickerPriceClear(ele){
		 	if($("#house_building_area1").val()==''){
	 			$("#transaction_fee_is").val('');
	 		}
			$("#tax_amount_is").val('');
			$("#tax_amount_no").val('');
		 	$("#personal_income_tax_is").val('');
			$("#personal_income_tax_no").val('');
			$("#deed_tax_is_one").val('');
			$("#deed_tax_is_two").val('');
			$("#deed_tax_no").val('');
			$("#land_appreciation_tax_no").val('');
			$("#transaction_fee_no").val('');
			$("#transaction_price").val('');
			$("#transaction_price1").val('');
			$("#six_amount").val('');
			$("#nine_amount").val('');
		 	$("#total_amount").val('');
			sticker_price = '';
	 }
	 //计算税费小计
	 function calculateTax(ele){
		 taxCheck(ele);
		 var tax = $("#tax_amount").val();
		 var personnal = $("#personal_income_tax").val();
		 var deed = $("#deed_tax").val();
		 var land = $("#land_appreciation_tax").val();
		 var fee = $("#transaction_fee").val();
		 var basic = $("#basic_price").val();
		 if(tax!=''&&personnal!=''&&deed!=''&&land!=''&&fee!=''&&basic!=''){
			 total_tax_amount = parseFloat(tax)+parseFloat(personnal)+parseFloat(deed)+parseFloat(land)+parseFloat(fee)+parseFloat(basic);
			 $("#total_tax_amount").val(total_tax_amount.toFixed(0));
			 if( $("#sticker_price").val()!=''){
				 $("#transaction_price").val((sticker_price*0.95-total_tax_amount).toFixed(0));
				 $("#transaction_price1").val((sticker_price*0.95-total_tax_amount).toFixed(0));
				 $("#six_amount").val(((sticker_price*0.95-total_tax_amount)*0.6).toFixed(0));
				 $("#nine_amount").val(((sticker_price*0.95-total_tax_amount)*0.9).toFixed(0));
	 			 $("#total_amount").val((sticker_price*0.95-total_tax_amount).toFixed(0));
			 }
		 }else{
			 $("#total_tax_amount").val('');
			 total_tax_amount = '';
		 }
	 }
	 //清空与税费小计相关数据
	 function toTaxClear(ele){
		 $(ele).focus();
	 	 $("#transaction_price").val('');
		 $("#transaction_price1").val('');
	 	 $("#six_amount").val('');
		 $("#nine_amount").val('');
		 $("#total_amount").val('');
	 	 total_tax_amount = '';
	 }
	 
