	//保存
	function save(pass,advice,flag) {
		$('#zcbtn').css('display','none');
		$('#tjbtn').css('display','none');
		$('#cancelBtnId').css('display','none');
		var jsonStr={};
		globalUtil.getFormParam('yfdinfo', jsonStr);
		globalUtil.getFormParam('yfpginfo', jsonStr);
		getOtherVal(jsonStr);
		if(flag == 1){
			jsonStr.pass=pass;
			jsonStr.advice=advice;
			jsonStr.taskId=taskId;
		}	
		jsonStr.wms_cre_credit_head_id=wms_cre_credit_head_id;
		jsonStr.fileArr = JSON.stringify(fileArr);//附件
		$.post(globalUtil.getTimestampUrl("/loancheck/wmscrecarchecksavenew.do"),
		   jsonStr, function(data) {
		   		if (data === 'success') {
           			globalUtil.successMsg(globalErrorMsg['100002'],
           				function(){            		 		
               				refreshAndClosePage();
           			});//保存成功  
		   		}else if(data === 'temOK'){
		   			globalUtil.successMsg(globalErrorMsg['100032'],
	           				function(){            		 		
	               				refreshAndClosePage();
	           			});
		   		}else if(data === 'supply'){
		   			globalUtil.errorMsg(globalErrorMsg['100040'],
	           				function(){            		 		
	               				refreshAndClosePage();
	           			});//该单据已经退件
           		}else {
               		globalUtil.errorMsg(globalErrorMsg['100012']); //保存失败
           		}
		});
	}
	
	function checkArr(commarr, val) {
		for (var i = 0; i < commarr.length; i++) {
			if (val == commarr[i]) {
				return true;
			}
		}
		return false;
	}

