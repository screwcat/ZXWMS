	//用于onblur事件，且不可以等于0 检查挂牌单价和面积
	function stickerPriceCheck(ele){
		var val = $(ele).val();
	 	if(val!='' && (globalUtil.isNum(val.substring(0,val.length-2)) || val.substring(0,val.length-2)==0) && val.substring(val.length-1)=='.'||val==null||val=='0'){
