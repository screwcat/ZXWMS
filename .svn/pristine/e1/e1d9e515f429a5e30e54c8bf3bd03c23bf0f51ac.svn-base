<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-
transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="../../css/zx-all.css" rel="stylesheet" type="text/css" />
<link href="../../css/app.css" rel="stylesheet" type="text/css" />
<link href="../../css/ligerui-main.css" rel="stylesheet" type="text/css" />
<script src="../../js/zx-all.js" type="text/javascript"></script>
<title>开资情况审核结果的流程历程查看</title>
<style type="text/css">
.td_title {
	background: none repeat scroll 0 0 #EAF0FB;
}
.tb_input TD {
	background: none repeat scroll 0 0 #FFFFFF;
	padding: 3px 1px 1px 4px;
}
</style>
<script type="text/javascript">
    var wms_inve_salary_pre_total_id;
	$(function() {
		wms_inve_salary_pre_total_id=window.parent.wms_inve_salary_pre_total_id
	});
</script>
</head>
<body>
	<hr />
	<div>
		<div class="center-title">审批流程</div>
	</div>
	<hr />
	<div id="grid"></div>
	<!-- 提交功能按钮区 -->
	<div class="pop-footer5 clearboth" style="bottom: 1px; height: 35px" id="tb_btn">
	        <input id="cancelBtnId"
			class="btn-cancel" onmouseover="this.className='btn-cancel-over'"
			onmouseout="this.className='btn-cancel'"
			onmousedown="this.className='btn-cancel-down'" type="button"
			onclick="closeDialog();" />
	</div>
	
	<!-- 工具条初始化 -->
	<script type="text/javascript">
		var grid;
		var dialog;
		var columns;
		$(function() {
			initGrid();
			search();
		});
		var dialog;
		function initGrid() {
			columns = [ {
				display : '节点名称',
				name : 'taskName',
				width : 140,
				minWidth : 140
			}, {
				display : '审批人',
				name : 'approvers',
				width : 100,
				minWidth : 100
				
			}, {
				display : '审批人部门',
				name : 'personnel_deptName',
				width : 150,
				minWidth : 150
			}, {
				display : '职务',
				name : 'personnel_postName',
				width : 100,
				minWidth : 100
			},{
				display : '审批结果',
				name : 'approveResult',
				width : 100,
				minWidth : 100
			}, {
				display : '审批意见',
				name : 'approveAdvice',
				width : 180,
				minWidth : 180
			}, {
				display : '审批时间',
				name : 'approveTime',
				width : 180,
				minWidth : 180
			} ];

			grid = $("#grid").ligerGrid({
				columns : columns,
				url : global_param.context_name + "/inve/getSalarySetProcessInfos.do?wms_inve_salary_pre_total_id="+wms_inve_salary_pre_total_id,
				rownumbers : true,
				allowUnSelectRow : true,
				usePager : false,
				width : '100%',
				height : 350,
				heightDiff : -4,
				parms : {
					_filterParms : -1,
				}
			});
		}
		//查询
		function search() {
			global_ligerui_extend.search(grid);
		}
		function closeDialog(){
			window.parent.dialog.hide();
		}
	</script>
</body>
</html>
