	//更改主贷人时，查询车贷信息
	function checkCarInfoWhIsmajChanged(mccidnwchged) {
		//现将抵押房贷信息grid第一行更新
		carInfoReset();
		if(mccidnwchged!=''){
			var len = modifyJsonCus.length;//得到手动更改的客户信息的数量
			var isInModJsonStr=false;//标记主贷人是否在手动更改的客户信息里
			//如果有更改的客户信息
			if(len>0){
				for(var i =len-1; i>=0; i--){
					var data = modifyJsonCus[i];
					if(mccidnwchged == data.wms_cus_customer_id){
						isInModJsonStr=true;
						var harr=JSON.parse(data.carstr)
						//如果在 并且只有一条车贷信息 则更新抵押车贷信息
						if(harr.length==1){
							mcclcid=harr[0].wms_cus_customer_line_carpinfo_id;
							
							griddycc.updateRow(0,harr[0]); 
						}else {
							mcclcid='';
						}
						break;
					}
				}
				//如果不在，则查询数据库中该客户的房贷信息
				if(!isInModJsonStr){
					var queryCarData = globalUtil.syncGetJson('/cusmanage/wmscuscustomerlinecarpinfowithoutpaginglist.do',{
						'wms_cus_customer_id': mccidnwchged // 
		                },'GET');
					if(queryCarData.Rows.length==1){
						mcclcid=queryCarData.Rows[0].wms_cus_customer_line_carpinfo_id;
						griddycc.updateRow(0,queryCarData.Rows[0]); 
					}else {
						mcclcid='';
					}
				}
			}else {
				//如果没有修改过的客户，则查询数据库中的记录
				var queryCarData = globalUtil.syncGetJson('/cusmanage/wmscuscustomerlinecarpinfowithoutpaginglist.do',{
					'wms_cus_customer_id': mccidnwchged // 
	                },'GET');
				if(queryCarData.Rows.length==1){
					mcclcid=queryCarData.Rows[0].wms_cus_customer_line_carpinfo_id;
					griddycc.updateRow(0,queryCarData.Rows[0]); 
				}else {
					mcclcid='';
				}
			}
		}else {
			mcclcid='';
		}
	} 
	
	//更改主贷人时，查询房贷信息
	function checkHouseInfoWhIsmajChanged(mccidnwchged){
		//现将抵押房贷信息grid第一行更新
		houseInfoReset();
