	function creLoanTypeMsg(cre_loan_type, housedata) {
		if(cre_loan_type == "2" || cre_loan_type == "3") {
			if(housedata['0'].wms_cus_customer_line_houseinfo_id == null || housedata['0'].wms_cus_customer_line_houseinfo_id == "") {
