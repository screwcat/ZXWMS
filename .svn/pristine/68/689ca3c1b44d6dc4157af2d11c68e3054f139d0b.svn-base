	//联动合同编号(年份)
	function protocol_id_year(){
		var val=$('#protocol_id_year').val();
		$('#protocol_id_year1').val(val);
		$('#protocol_id_year2').val(val);
		var num=$('#protocol_id_num').val();
		var yearnum=val+"年第"+num+"号";//补充协议中的合同编号
		$('#protocol_id_year_num').val(yearnum);
		
	}
	function protocol_id_yearone(){
		var val=$('#protocol_id_year1').val();
		$('#protocol_id_year').val(val);
		$('#protocol_id_year2').val(val);
	}
	function protocol_id_yeartow(){
		var val=$('#protocol_id_year2').val();
		$('#protocol_id_year').val(val);
		$('#protocol_id_year1').val(val);
	}
	//联动合同编号(号)
	function protocol_id_num(){
		var val=$('#protocol_id_num').val();
		$('#protocol_id_num1').val(val);
		$('#protocol_id_num2').val(val);
		var year=$('#protocol_id_year').val();
		var yearnum=year+"年第"+val+"号";//补充协议中的合同编号
		$('#protocol_id_year_num').val(yearnum);
	}
	function protocol_id_numone(){
		var val=$('#protocol_id_num1').val();
		$('#protocol_id_num').val(val);
		$('#protocol_id_num2').val(val);
	}
	function protocol_id_numtow(){
		var val=$('#protocol_id_num2').val();
		$('#protocol_id_num').val(val);
		$('#protocol_id_num1').val(val);
	}
