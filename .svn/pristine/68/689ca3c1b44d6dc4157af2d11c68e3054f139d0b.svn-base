/****************页面上的金额**********************/
//初始化每月还款利息
function setRefund_interest_month_lower(jsonForSys){
	 jsonForSys.refund_interest_month_lower=Math.round(parseFloat(jsonForSys.principal_lower)*parseFloat(jsonForSys.borrow_interest)*0.01);
}
//初始化每月还款总计
function setRefund_limit_month_lower(jsonForSys){
	 jsonForSys.refund_limit_month_lower=parseFloat(jsonForSys.refund_interest_month_lower)+parseFloat(jsonForSys.refund_principal_month_lower);
}
//初始化每月还款金额该数值 = 借款合同内的借款本金 * （借款月利率+居间服务利率），四舍五入取整。borrow_interest
function setRefund_limit_month(jsonForSys){
	 jsonForSys.refund_limit_month=Math.round(parseFloat(jsonForSys.principal_lower)*(parseFloat(jsonForSys.service_cost_month)+parseFloat(jsonForSys.borrow_interest))*0.01);
}
//初始化违约金 借款合同内的借款本金 * 每日违约金利率。系统提供默认值，现阶段可手工修改。其中每日违约金利率可后台配置，房贷=0.1%。
function setLiquidated_damages(jsonForSys){
	 jsonForSys.liquidated_damages=Math.round(parseFloat(jsonForSys.principal_lower)*parseFloat(jsonForSys.weiyuejin)*0.01);
}
//初始化居间服务费    居间服务费月利率 * 贷款期数 。
function setJujian_service_cost(jsonForSys){
	 jsonForSys.jujian_service_cost=parseFloat(jsonForSys.service_cost_month*1000000)*parseFloat(jsonForSys.borrow_deadline)/1000000;
}
