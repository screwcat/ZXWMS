	    			$('#prompt_date').text(json.prompt_date);	
	    			$('[name=prot_code]').val(json.protocolInfo[0].bill_code);
	    			$("#prot_code").text(json.protocolInfo[0].bill_code);
	    			$("#financial_bill_code").text(json.protocolInfo[0].financial_bill_code);
	    			$('[name=b_name]').val(json.protocolInfo[0].b_name);
	    			$('[name=b_id_card]').val(json.protocolInfo[0].b_id_card);
	    			$('[name=bonus_rate]').val(json.protocolInfo[0].bonus_rate);
	    			$('[name=product_account]').val(parseFloat(json.protocolInfo[0].product_account)/10000);
	    			$('[name=category_name]').val(json.protocolInfo[0].category_name);
	    			$('[name=product_deadline_name]').val(json.protocolInfo[0].product_deadline_name);
	    			$('[name=product_interest]').val(json.protocolInfo[0].show_product_interest);
	    			$('[name=create_timestamp_str]').val(json.protocolInfo[0].date_of_payment);
	    			$('[name=end_of_date]').val(json.protocolInfo[0].end_of_date);
	    			$('[name=over_date_value]').val(json.protocolInfo[0].over_date_value);
	    			$('[name=redeem_date]').val(now_date);
	    			$('[name=card_no]').val(json.protocolInfo[0].card_no);
	    			$('[name=bank_of_deposit]').val(json.protocolInfo[0].bank_of_deposit+" | "+json.protocolInfo[0].bank_of_deposit_pro+json.protocolInfo[0].bank_of_deposit_city+json.protocolInfo[0].bank_branch);
	    			$('[name=protocol_create_timestamp]').val(json.protocolInfo[0].protocol_create_timestamp);

	    			wms_inve_transa_prod_id = json.protocolInfo[0].wms_inve_transa_prod_id;

	    			//初始化页面（到期/未到期）
	    			init_redeem_div();
	    			
	    		});
    }


	//验证正整数
	 function valiInt(ele){
	 	var val = $(ele).val();
	 	if(val!='' && !(globalUtil.isNum(val))){
	 		errorMsg(globalErrorMsg['400106'],ele);
	 	}
	 }
	//验证正整数，可以等于0
	function valiInt1(ele){
		var val = $(ele).val();
		var res = /^(-?((0|[1-9]+)|([1-9]\d*)))?$/.test(val)
		if (val!='' && !res) {
			errorMsg(globalErrorMsg['400114'],ele);
        }
	}
	//验证正小数，用于onkeyup事件
	 function valiFloat(ele){
	 	var val = $(ele).val();
	 	if(val!='' && !(globalUtil.isFloat(val) || (globalUtil.isNum(val.substring(0,val.length-2))||val.substring(0,val.length-2)==0) && val.substring(val.length-1)=='.')){
	 		$(ele).val('');
	 		$(ele).focus();
	 		globalUtil.errorMsg(globalErrorMsg['400107'],function(ele){
	 			toIncomeManageFeeClear(ele);
	 		});
	 	}
	 }
	function checkFloat(val){
		if(val!='' && !(globalUtil.isFloat(val) || (globalUtil.isNum(val.substring(0,val.length-2))||val.substring(0,val.length-2)==0) && val.substring(val.length-1)=='.')){
			return false;
	 	}
		return true;
	}
	function checkInt(val){
		var res = /^(-?((0|[1-9]+)|([1-9]\d*)))?$/.test(val)
		if (val!='' && !res) {
			return false;
        }
		return true;
	}
	
	//用于onblur事件
	 function valiFloat2(ele){
		var redeem_amount = 0;
		 $("input[name=redeem_amount]").each(function(i){
