		LEFT JOIN wms_cre_credit_group t4 ON t1.wms_cre_credit_group_id = t4.wms_cre_credit_group_id
		LEFT JOIN wms_fina_cre_loan_app t5 ON t5.wms_cre_credit_head_id =
		t1.wms_cre_credit_head_id
		WHERE
		t1.enable_flag=1
		AND t3.is_major=1
