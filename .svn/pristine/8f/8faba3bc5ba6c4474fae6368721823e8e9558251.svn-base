/*--submenu---*/
.submenuDiv {
height:auto; width:auto; min-height:135px; overflow:hidden; white-space:nowrap; padding:10px 20px; display:none; position:absolute; top:78px; background:url(../images/index/submenu_bg.png) left top repeat;	
}

.submenuDiv .submenuUl {float:left; margin-right:20px;}
.submenuDiv .submenuUl .subLi {color:#55c9ff; background:url(../images/index/point01.gif) 0 7px no-repeat; text-indent:16px;}
.submenuDiv .subLi li a {color:#fff; text-decoration:none; cursor:pointer;}
.submenuDiv .subLi li a:hover{text-decoration:underline;}


/*--shortcut---*/
#shortcut {width:1200px; margin:0 auto; height:auto; padding:20px;}
.btn-addclient, .btn-addclient-over, .btn-addclient-down, .btn-addloan, .btn-addloan-over, .btn-addloan-down, .btn-warReport, .btn-warReport-over, .btn-warReport-down,  
.btn-alreadydo, .btn-alreadydo-over, .btn-alreadydo-down, .btn-rules, .btn-rules-over, .btn-rules-down, .btn-todolist, .btn-todolist-over, .btn-todolist-down {
	background-image:url(../images/btn/shortcut.png);
	background-color:transparent;
	background-repeat:no-repeat;
	width:156px;
	height:156px;
	border-width:0;
	/*padding-left:15px;*/
	cursor:pointer;
}
/*新增客户*/
.btn-addclient {background-position:0 0;}
.btn-addclient-over, .btn-addclient-down {background-position:0 -156px;}
/*新增贷款*/
.btn-addloan {background-position:0 -312px;}
.btn-addloan-over, .btn-addloan-down {background-position:0 -468px;}
/*预警报表*/
.btn-warReport {background-position:0 -624px;}
.btn-warReport-over, .btn-warReport-down {background-position:0 -780px;}
/*已办事宜*/
.btn-alreadydo {background-position:0 -936px;}
.btn-alreadydo-over, .btn-alreadydo-down {background-position:0 -1092px;}
/*规章制度*/
.btn-rules {background-position:0 -1248px;}
.btn-rules-over, .btn-rules-down {background-position:0 -1404px;}
/*待办事宜*/
.btn-todolist {background-position:0 -1560px;}
.btn-todolist-over, .btn-todolist-down {background-position:0 -1716px;}

