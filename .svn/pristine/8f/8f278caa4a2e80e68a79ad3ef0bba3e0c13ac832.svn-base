		$(".borrow_page").show();
		document.execCommand("print");
		$("#dybtn").show();
		$(".borrow_page").hide();
		return;			
	 }
	 var jsonStr = globalUtil.getFormParam('zrxy');
		  jsonStr.wms_inve_transa_id=wms_inve_transa_id;
		  jsonStr.wms_inve_transa_prod_id=wms_inve_transa_prod_id;
