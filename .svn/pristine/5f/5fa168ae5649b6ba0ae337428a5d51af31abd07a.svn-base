		t1.bill_code,
		t1.credit_purpose,
		t1.max_repayment_limit_per_month,
		(case t1.cre_type when '3' then (CAST(t1.credit_limit*0.0001 AS DECIMAL(8,4) )) else t1.credit_limit end) as credit_limit,
		t1.max_repayment_time_limit,
