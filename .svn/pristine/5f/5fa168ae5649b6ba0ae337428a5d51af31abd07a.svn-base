		and wms_sys_dict_id =24)) end)as bill_status_name,
		
		t1.water_appro_user_id,
		t1.water_appro_user_name,
		date_format(t1.water_appro_timestamp,'%Y-%m-%d %H:%i:%s') as water_appro_timestamp,
