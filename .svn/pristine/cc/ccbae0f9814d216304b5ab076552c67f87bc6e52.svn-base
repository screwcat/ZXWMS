/* 
 * My97 DatePicker 4.7
 */

.WdateDiv {
	width:180px;
	width:186px\9;
	background:#c7e6fd url(dateDiv-bg.gif) repeat-x;
	border:#99c1e0 1px solid;
	padding:2px;
}
.WdateDiv {font-family:Arial,'宋体',Helvetica, sans-serif; font-size:8px;}
.WdateDiv2 {
	width:360px;
}
.WdateDiv * {
	font-size:9pt;
}
.WdateDiv .NavImg a {
	display:block;
	cursor:pointer;
	height:16px;
	width:16px;
}
.WdateDiv .NavImgll a {
	float:left;
	background:transparent url(img.gif) no-repeat scroll 0 2px;
}
.WdateDiv .NavImgl a {
	float:left;
	background:transparent url(img.gif) no-repeat scroll -16px 2px;
}
.WdateDiv .NavImgr a {
	float:right;
	background:transparent url(img.gif) no-repeat scroll -32px 2px;
}
.WdateDiv .NavImgrr a {
	float:right;
	background:transparent url(img.gif) no-repeat scroll -48px 2px;
}
.WdateDiv #dpTitle {
	height:24px;
	margin-bottom:2px;
	padding:1px;
}
.WdateDiv .yminput {
	margin-top:0;
	text-align:center;
	height:20px;
	border:0px;
	width:50px;
	cursor:pointer;
	background-color:transparent;
	vertical-align:middle;
}
.WdateDiv .yminputfocus {
	margin-top:2px;
	text-align:center;
	font-weight:bold;
	height:20px;
	color:blue;
	border:#ccc 1px solid;
	width:50px;
}
.WdateDiv .menuSel {
	z-index:1;
	position:absolute;
	background-color:#FFFFFF;
	border:#ccc 1px solid;
	display:none;
}
.WdateDiv .menu {
	cursor:pointer;
	background-color:#fff;
}
.WdateDiv .menuOn {
	cursor:pointer;
	background-color:#c7f2ce;
}
.WdateDiv .invalidMenu {
	color:#aaa;
}
.WdateDiv .YMenu {
	margin-top:20px;
}
.WdateDiv .MMenu {
	margin-top:20px;
 *width:62px;
}
.WdateDiv .hhMenu {
	margin-top:-90px;
	margin-left:26px;
}
.WdateDiv .mmMenu {
	margin-top:-46px;
	margin-left:26px;
}
.WdateDiv .ssMenu {
	margin-top:-24px;
	margin-left:26px;
}
.WdateDiv .Wweek {
	text-align:center;
	background:#DAF3F5;
	border-right:#BDEBEE 1px solid;
}
.WdateDiv .MTitle {
 background:#f7f7f7 url(week-bg.gif) repeat-x;
}
.WdateDiv .WdayTable2 {
	border-collapse:collapse;
	border:#c5d9e8 1px solid;
}
.WdateDiv .WdayTable2 table {
	border:0;
}
.WdateDiv .WdayTable {
	line-height:20px;
	border:#a3c9e6 1px solid;
	background:#fffcf7;
}
.WdateDiv .WdayTable td {
	text-align:center;
}
.WdateDiv .Wday {
	cursor:pointer;
}
.WdateDiv .WdayOn {
	cursor:pointer;
	background-color:#e0f6e4;
}
.WdateDiv .Wwday {
	cursor:pointer;
	color:#FF2F2F;
}
.WdateDiv .WwdayOn {
	cursor:pointer;
	color:#000;
	background-color:#C0EBEF;
}
.WdateDiv .Wtoday {
    cursor:pointer;
    color:#0091ff; 
    font-weight:bold;
}
.WdateDiv .Wselday {
	background-color:#c7f2ce;
}
.WdateDiv .WspecialDay {
	background-color:#66F4DF;
}
.WdateDiv .WotherDay {
	cursor:pointer;
	color:#b9b9b9;
}
.WdateDiv .WotherDayOn {
	cursor:pointer;
	background-color:#C0EBEF;
}
.WdateDiv .WinvalidDay {
	color:#aaa;
}
.WdateDiv #dpTime {
	float:left;
	margin-top:1px;
	margin-right:10px;
}
.WdateDiv #dpTime #dpTimeStr {
	margin-left:1px;
	line-height:18px;
	vertical-align:middle;
}
.WdateDiv #dpTime input {
	width:18px;
	height:20px;
	text-align:center;
	border:#a3c9e6 1px solid;
	margin-top:2px\9;
	line-height:18px;
}
.WdateDiv #dpTime .tB {
	border-right:0px;
}
.WdateDiv #dpTime .tE {
	border-left:0;
	border-right:0;
}
.WdateDiv #dpTime .tm {
	width:7px;
	border-left:0;
	border-right:0;
}
.WdateDiv #dpTime #dpTimeUp {
	height:10px;
	width:13px;
	border:0px;
	background:url(img.gif) no-repeat -32px -16px;
	margin-top:2px\9;
	cursor:pointer;
}
.WdateDiv #dpTime #dpTimeDown {
	height:10px;
	width:13px;
	border:0px;
	background:url(img.gif) no-repeat -48px -16px;
	cursor:pointer;
}
.WdateDiv #dpQS {
	float:left;
	margin-right:3px;
	margin-top:3px;
	margin-top:5px\9;
	background:url(img.gif) no-repeat 0px -16px;
	width:20px;
	height:20px;
	height:18px\9;
	cursor:pointer;
}
.WdateDiv #dpControl {
	text-align:left;
	width:160px;
	width:150px\9;
}
.WdateDiv .dpButton {
	width:43px;
	border:none;
	background:url(dpButton-bg.gif) no-repeat center;
	margin-top:1px;
	margin-top:3px\9;
	margin-left:2px;
	height:19px;
	padding-left:1px\9;
	padding-top:1px\9;
	color:#fff;
	text-align:center;
	cursor:pointer;
}

.dpButton[id="dpTodayInput"]:disabled {display:none }