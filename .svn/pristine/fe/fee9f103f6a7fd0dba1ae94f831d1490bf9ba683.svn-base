	//获取企业信息
	function getFromValues(){
		var  resultJson={};//存储所有
		var  qybaseInfo=[];//企业基本情况
		$('.class_qy_base').each(function(i){
			var qyJson={};
			qyJson.has_enterprise=$(this).find('[name ^=has_enterprise]:checked').get(0).value;//营业执照查询结果
			qyJson.enterprise_reason=$(this).find('[name=enterprise_reason]').get(0).value;//原因
			qyJson.company_type=$(this).find('[name=company_type]').get(0).value;//企业类型
			qyJson.operation_state=$(this).find('[name=operation_state]').get(0).value;//经营状态
			qyJson.register_money=$(this).find('[name=register_money]').get(0).value;//注册资金
			qyJson.date_of_issue=$(this).find('[name=date_of_issue]').get(0).value;//发布日期
			qyJson.issue_begin_date=$(this).find('[name=issue_begin_date]').get(0).value;//营业开始时间
			qyJson.issue_end_date=$(this).find('[name=issue_end_date]').get(0).value;//营业截止时间
			qyJson.shareholders_num=$(this).find('[name=shareholders_num]').get(0).value;//股东个数			
			var way_of_contribution_arr=[];
			$(this).find('[name ^=way_of_contribution]:checked').each(function(j){
				way_of_contribution_arr.push($(this).val());
			});
			qyJson.way_of_contribution=way_of_contribution_arr.join('#');//出资方式
			qyJson.is_inspection=$(this).find('[name ^=is_inspection]:checked').get(0).value;//是否年检
			qyJson.is_same_organ_code=$(this).find('[name ^=is_same_organ_code]:checked').get(0).value;//机构登记号是否一致
			qyJson.is_inspection_organ_code=$(this).find('[name ^=is_inspection_organ_code]:checked').get(0).value;//机构登记证是否年检
			qyJson.is_same_other=$(this).find('[name ^=is_same_other]:checked').get(0).value;//其它相关信息是否一致：
		    qyJson.is_taxpayer=$(this).find('[name ^= is_taxpayer]:checked').get(0).value;//是否具有一般纳税人资格
		    qyJson.pay_taxes_status=$(this).find('[name=pay_taxes_status]').get(0).value;//纳税状态
		    qyJson.comp_remark=$(this).find('[name=comp_remark]').get(0).value;//特殊说明
		    qybaseInfo.push(qyJson);
		});
		//获取企业信息法院网执行案件有无
		var ztqkJson={};
		ztqkJson.comp_court_case=getRadioValues('comp_court_case');//企业法院网有无判断
		var info_qyfyw=[];
		if(ztqkJson.comp_court_case=='1'){
			$('.class_xxqy_fyw').each(function(i){
				var qyfywJson={};
				qyfywJson.court_case_type="2";//案件类型
				qyfywJson.execute_status=$(this).find('[name ^=execute_status]:checked').get(0).value;//执行状态
				qyfywJson.accreditation_date=$(this).find('[name=accreditation_date]').get(0).value;//立案时间
				qyfywJson.execute_target=$(this).find('[name=execute_target]').get(0).value;//执行标的
				qyfywJson.court_case_remark=$(this).find('[name=court_case_remark]').get(0).value;//特殊说明
				info_qyfyw.push(qyfywJson);
			});
		}else{
			$('.class_xxqy_fyw').each(function(i){
				var qyfywJson={};
				qyfywJson.court_case_type="2";//案件类型
				qyfywJson.court_case_remark=$(this).find('[name=court_case_remark]').get(0).value;//特殊说明
				info_qyfyw.push(qyfywJson);
			});
		}
		resultJson.qybaseInfo=qybaseInfo;//企业基本信息
		resultJson.ztqkJson=ztqkJson;//存储企业法院网有无
		resultJson.info_qyfyw=info_qyfyw;//存储企业法院网案件
		return resultJson;
	}
	
	
	
	
	//----------------------------------------------------------------------------------------------------------------------------------------
	//设置checkBox的Values
	function setCheckBoxValues(nameVal,valFromDb,ele,flag){
		if(flag){
			$(ele).find("input[name^='"+nameVal+"']").each(function(i) {
				var obj = $(this);
				for(var j = 0;j<valFromDb.length;j++){
					if(obj.val() == valFromDb[j]){
						obj.attr('checked', 'checked');
					}
				}
				/*
				if ($(this).val() == valFromDb[i]) {
					$(this).attr('checked', 'checked');
				}*/
			});
			
		}else{
			$("input[name='"+nameVal+"']").each(function(i) {
				var obj = $(this);
				for(var j = 0;j<valFromDb.length;j++){
					if(obj.val() == valFromDb[j]){
						obj.attr('checked', 'checked');
					}
				}
			});
		}	
	}
	//设置Radio控件的值
	function setRadioVal(nameVal,valFromDb,ele,flag){
		if(flag){
			$(ele).find("input[name ^='"+nameVal+"']").each(function() {
				if ($(this).val() == valFromDb) {
					$(this).attr('checked', 'checked');
					return false;
				}
			});
			
		}else{
			$("input[name='"+nameVal+"']").each(function() {
				if ($(this).val() == valFromDb) {
					$(this).attr('checked', 'checked');
					return false;
				}
			});
		}	
	}
	//根据name或class获取控件的值(适合没有新增)
	function getValue(nameValue,classValue){
		if(classValue==null){
			return document.getElementsByName(nameValue)[0].value;
		}else{
			return $('.'+classValue).val();
		}
	}
	//获取多选按钮的值选中的值
	function getCheckBoxValues(nameValue,classValue){
		var result=[];
		if(classValue==null){
			var eleArr = document.getElementsByName(nameValue);
			$.each(eleArr,function(i,ele){
				if($(ele).attr('checked')){
					result.push($(ele).val()) ;
				}
			});
		}else{
			var eleArr = $('.'+classValue);
			$.each(eleArr.get(),function(i,ele){
				if($(ele).attr('checked')){
					result.push($(ele).val()) ;
				}
			});
		}
		return result;
	}
	//获取单选按钮的值
	function  getRadioValues(nameValue,classValue){
		var result;
		if(classValue==null){
			var eleArr = document.getElementsByName(nameValue);
			$.each(eleArr,function(i,ele){
				if($(ele).attr('checked')){
					result = $(ele).val();
					return false;
				}
			});
		}else{
			var eleArr = $('.'+classValue);
			$.each(eleArr.get(),function(i,ele){
				if($(ele).attr('checked')){
					result = $(ele).val();
					return false;
				}
			});
		}
		return result;
	}
