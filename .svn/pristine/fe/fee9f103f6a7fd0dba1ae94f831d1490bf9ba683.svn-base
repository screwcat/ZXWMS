	//公共删除本组信息
	function delThisInfo(ele){
		globalUtil.confirmMsg(globalErrorMsg['100035'],
		    	function(yes) { //确认删除
		    			if(yes){
		    				$(ele).parent().parent().parent().remove();
		    		}
		    });	
	}
	//企业法院网案件执行情况有无判断
	function toggleqyfyw(ele){
		if($(ele).val()==1 && $(ele).attr('checked')){
			//判断有
			$('#toolbar-qy-fyw').css('display','');
			$.each($(ele).parent().parent().find('.class_xxqy_fyw').get(),function(i,inele){
			$(inele).find("input").removeAttr("disabled","disabled");
			$(inele).find("textarea").removeAttr("disabled","disabled");
			});
		}else if($(ele).val()==0 && $(ele).attr('checked')){
			//判断无
			$('#toolbar-qy-fyw').css('display','none');
			$.each($(ele).parent().parent().find('.class_xxqy_fyw').get(),function(i,inele){
				$(inele).find("input").attr("disabled","disabled");
				$(inele).find("input").val('');
			});
		}
	}
	
	//设置为只读模式
	function setReadOnly(){
		$("input[type=text]").attr({
			"disabled" : "disabled"
		});
		$("input[type=radio]").attr({
			"disabled" : "disabled"
		});	
		$("select").attr({
			"disabled" : "disabled"
		});
		$("textarea").attr({
			"disabled" : "disabled"
		});
		$("input[type=checkbox]").attr({
			"disabled" : "disabled"
		});
		$(".minwidth400").remove();
	}
	//-----------------------------------------------------------------------------------------------------
	//验证非负整数
	function valiInt(ele){
		var val = $(ele).val();
		if(val!='' && !(globalUtil.isNum(val) || val==0)){
			errorMsg(globalErrorMsg['400101'],ele);
		}
	}
	//验证非负小数，用于onkeyup事件
	 function valiFloat(ele){
	 	var val = $(ele).val();
	 	if(val!='' && !(globalUtil.isFloat(val) || (globalUtil.isNum(val.substring(0,val.length-2))||val.substring(0,val.length-2)==0) && val.substring(val.length-1)=='.')){
	 		errorMsg(globalErrorMsg['400107'],ele);
	 	}
	 }
	//用于onblur事件
	 function valiFloat2(ele){
	 	var val = $(ele).val();
	 	if(val!='' && (globalUtil.isNum(val.substring(0,val.length-2)) || val.substring(0,val.length-2)==0) && val.substring(val.length-1)=='.'){
	 		errorMsg(globalErrorMsg['400107'],ele);
	 	}
	 }
	//验证日期
	function valiDay(ele){
		var val = $(ele).val();
		if(val!='' && !(globalUtil.isNum(val) && val>0 && val<32)){
			errorMsg(globalErrorMsg['400103'],ele);
		}
	}
	//验证尾号
	function valiEndNo(ele){
		var val = $(ele).val();
		if(val!='' && !(/^[0-9]+$/.test(val))){
			errorMsg(globalErrorMsg['400104'],ele);
		}
	}
	//弹出错误提示框
	function errorMsg(msg,ele){
		globalUtil.addMask('1');
		var obj = $.ligerDialog.error(msg, null, null);
		obj.options.onHiddenOrClose = function() {
			$(ele).val('');
			$(ele).focus();
			globalUtil.delMask('1');
	    };
	}
