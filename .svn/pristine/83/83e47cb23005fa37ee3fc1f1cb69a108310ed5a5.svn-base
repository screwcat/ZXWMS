<script type="text/javascript">
var jsonStr;
var protocol_type="32";//合同编号--卓车贷-先息（不押车）
var htbhVal='';
var mcchiVal='';

$(function()  {
	$(".borrow_page").hide(); 
	var wms_cre_credit_head_id  =  $.query.get('wms_cre_credit_head_id');
	//初始化数据
	$.getJSON(globalUtil.getTimestampUrl('/loanappro/wmssyspropertyobjforhouse.do'),////初始化数据
		{
			"wms_cre_credit_head_id"  : wms_cre_credit_head_id
		},
		function(jsonForSys) {
			htbhVal=jsonForSys.protocol_id_year_num;
			mcchiVal=wms_cre_credit_head_id;
			if(jsonForSys.debtor_name!=null&&jsonForSys.debtor_name!=""){	
				if(jsonForSys.com_debtor_name!=null&&jsonForSys.com_debtor_name!=""){
					jsonForSys.debtor_name1=jsonForSys.debtor_name+" "+jsonForSys.com_debtor_name;	
				}else{
					jsonForSys.debtor_name1=jsonForSys.debtor_name;
				}
			}	
			globalUtil.setFormVal("jkht", jsonForSys);	
		});
	 	//设置为只读
		$("input[type=text]").attr({
			"disabled" : "disabled"
	});
});
function save(){
	globalUtil.syncGetJson('/loanappro/wmssyspropertysavelogforoprint.do',{'wms_cre_credit_head_id':mcchiVal,'htType':32,"htbhVal":htbhVal},'POST');
		$("#dybtn").hide();
	   	$(".borrow_page").show();
			document.execCommand("print");
		$("#dybtn").show();
		$(".borrow_page").hide(); 
}
</script>

