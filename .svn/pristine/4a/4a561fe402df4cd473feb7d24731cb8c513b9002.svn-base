@charset "utf-8";
/* 主页 */
*{font:Helvetica, Arial, "Microsoft YaHei", "宋体";}
body, html {
	height:100%;
}
body {
	margin:0;
	padding:0;
	/*background:#fcfcfc;*/
	ackground:#fff;
	overflow:hidden;
}
.l-main-page-layout {
	width:99.9%;
	margin:0;
	padding:0;
}
.l-page-top {
	height:60px;
	margin-bottom:3px;
	text-align:center;
}
div, ul, li, dt, dd, table, td, th {
	margin:0;
	padding:0;
}
.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: ".";
	clear: both;
	height: 0;
} /*-----for FF,Chrome-----*/
* html .clearfix {
	zoom: 1;
} /*----for ie6----*/
*:first-child + html .clearfix {
	zoom: 1;
}
a {color:#1382d7; text-decoration:underline;}
a:hover {color:#ff9200; text-decoration:underline;}
a:visited {color:#1382d7; text-decoration:underline;}
/*-----for ie7-----*/
#pageloading {
	position:absolute;
	left:0px;
	top:0px;
	/*background:white url('../../../../loading.gif') no-repeat center;*/
	width:100%;
	height:100%;
	z-index:99999;
}

