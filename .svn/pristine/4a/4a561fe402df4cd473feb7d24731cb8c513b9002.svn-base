/*--------通用按钮样式--------*/
.btnType1, .btnDisable, .btnType2 {margin:0; padding:0; border-radius:3px; text-align:center; font-size:12px;}

/*------Size------*/
.btnSize1 {width:76px; height:22px; line-height:22px;} /*--76x22--*/
.btnSize2 {width:114px; height:28px; line-height:28px;} /*--114x28--*/
.btnSize3 {width:120px; height:22px; line-height:22px;} /*--120x22--*/
.btnSize4 {width:160px; height:30px; line-height:30px; font-size:16px;} /*--160x30--*/

/*------Type-----*/
a.btnType1 {display:block; background:#0f81d7; border:1px solid #045ca7; color:#fff; text-decoration:none; text-shadow:0px -1px 0px #0c69b0; box-shadow:0px 1px 0px #bfc5c9; cursor:pointer;}
a.btnType1:hover {background:#1799e3; border:1px solid #045ca7; color:#fff; text-decoration:none; text-shadow:0px -1px 0px #0a75cd;}

a.btnType2 {display:block; background:#fff; border:1px solid #0f81d7; border-radius:15px; color:#0f81d7; text-decoration:none; text-shadow:none; box-shadow:0px 1px 0px #ebebeb; cursor:pointer;}
a.btnType2:hover {background:#c4e4ff; border:1px solid #c4e4ff; color:#0f81d7; text-decoration:none;}

/*-禁用按钮样式-*/
a.btnDisable {display:block; background:#c0c0c0; border:1px solid #c0c0c0; color:#fff; text-decoration:none; text-shadow:none; box-shadow:0px 1px 0px #bfc5c9; cursor:not-allowed;}

.pop-footer6 .btnType1 {margin-left:20px; margin-top:3px; display:inline-block;}
.pop-footer6 .btnDisable {margin-left:20px; margin-top:3px; display:inline-block;}
.position-re {position:relative;}













