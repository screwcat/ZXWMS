.l-tab-links-item-left-child {position:absolute;top:0; left:0; width:22px; height:28px; background:url(../images/layout/tabsItemLeft-child-selected.gif);}
.l-tab-links-item-right-child {position:absolute;top:0; right:0; width:19px; height:28px;background:url(../images/layout/tabsItemRight-child-selected.gif);}
.l-tab-links-item-close-child { width:1px; height:1px; overflow:hidden; position:absolute; top:2px; right:2px; z-index:12; cursor:pointer;}
.l-tab-links li.l-selected-child{background:url(../images/layout/tabsItemOver-child-bg.gif); color:#333; width:160px; font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体";}
.l-tab-links .l-unselected-child {background:url(../images/layout/tabsItemOver-child-bg.gif); color:#333; font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; width:160px;}
.l-tab-links .l-unselected-child a {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; color:#999; margin-left:30px;}
.l-tab-links .l-unselected-child a:hover {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; color:#f67d61;}
.l-tab-links .l-selected-child a {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; color:#333; margin-left:30px; margin-top:1px;}
.l-tab-links .l-selected-child a:hover {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; color:#333;}
.l-tab-links .l-selected-child a {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体";}
.l-unselected-child .l-tab-links-item-left-child {position:absolute;top:0; left:0; width:22px; height:28px; background:url(../images/layout/tabsItemLeft-child-unsel.gif);}
.l-selected-child .l-tab-links-item-left-child {position:absolute;top:0; left:0; width:22px; height:28px; background:url(../images/layout/tabsItemLeft-child-selected.gif);}
.l-selected-child .l-tab-links-item-close, .l-selected-child .l-tab-links-item-close-over {width:8px; height:8px; overflow:hidden; position:absolute; top:3px; right:13px; z-index:12; cursor:pointer; }
.l-selected-child .l-tab-links-item-close {background:url(../images/layout/tabs-child-close.gif) 0 0 no-repeat;}
.l-selected-child .l-tab-links-item-close-over{background:url(../images/layout/tabs-child-close-over.gif) 0 0 no-repeat;}
.l-unselected-child .l-tab-links-item-close, .l-unselected-child .l-tab-links-item-close-over {width:8px; height:8px; overflow:hidden; position:absolute; top:3px; right:13px; z-index:12; cursor:pointer; }
.l-unselected-child .l-tab-links-item-close {background:url(../images/layout/tabs-child-close.gif) 0 0 no-repeat;}
.l-unselected-child .l-tab-links-item-close-over{background:url(../images/layout/tabs-child-close-over.gif) 0 0 no-repeat;}

.l-tab-links-sub{position:relative; height:42px; width:100%; overflow:hidden; margin:auto 0px;}
.l-tab-links-sub-left{position:absolute; top:1px; left:0; width:17px; height:23px; overflow:hidden; background:url(../images/layout/tabs-tools.gif) 0px 0px; z-index:13; cursor:pointer;}
.l-tab-links-sub-right{ position:absolute; top:1px; right:0;width:17px; height:23px; overflow:hidden;background:url(../images/layout/tabs-tools.gif) -51px 0px;z-index:13; cursor:pointer;}
.l-tab-links-sub-left-over{ background-position:-17px 0px;}
.l-tab-links-sub-right-over{ background-position:-68px 0px;}
.l-tab-links-sub-left-invalid{ background-position:-34px 0px;}
.l-tab-links-sub-right-invalid{ background-position:-85px 0px;}
.l-tab-links-sub ul{ list-style:none; margin:0; padding:0; width:100%; height:28px; overflow:hidden; position:absolute; top:7px; left:0; bottom:7px; background-color:transparent;}
.l-tab-links-sub li{ float:left; margin:0 0 0 10px; padding:0; height: 28px; line-height:28px; cursor:pointer;background:url(../images/layout/tabs-item-over-bg.gif); position:relative; overflow:hidden; border:none; color:#197deb; }
.l-tab-links-sub li.l-selected{background:#def1ff; color:#197deb;}
.l-tab-links-sub li a{ display:block; margin:0 auto; margin-left:16px; margin-right:16px; text-align:center; font-weight:400; text-decoration:none; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体";}
.l-tab-links-sub li a:hover {color:#197deb;}
.l-tab-links-sub .l-selected a {color:#197deb;}
.l-tab-links-sub .l-selected a:hover {color:#197deb;}
.l-tab-links-item-left-sub {position:absolute;top:0; left:0; width:10px; height:28px; background:#def1ff;}
.l-tab-links-item-right-sub {position:absolute;top:0; right:0; width:10px; height:28px; background:#f0f0f0;}
.l-tab-links-item-close-sub { width:1px; height:1px; overflow:hidden; position:absolute; top:2px; right:2px; z-index:12; cursor:pointer;}
.l-tab-links-sub li.l-selected-sub{background:#def1ff; font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; }
.l-tab-links-sub .l-unselected-sub {background:#f0f0f0; color:#959697; font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体";}
.l-tab-links-sub .l-unselected-sub a {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; color:#959697;}
.l-tab-links-sub .l-unselected-sub a:hover {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; color:#197deb;}
.l-tab-links-sub .l-selected-sub a {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; color:#197deb;}
.l-tab-links-sub .l-selected-sub a:hover {font-weight:400; font-family:Helvetica, Arial, "Microsoft YaHei", "宋体"; color:#197deb;}
.l-unselected-sub .l-tab-links-item-left-sub {position:absolute;top:0; left:0; width:10px; height:28px; background:#f0f0f0;}
.l-selected-sub .l-tab-links-item-left-sub {position:absolute;top:0; left:0; width:10px; height:28px; background:#def1ff;}
.l-selected-sub .l-tab-links-item-right-sub {position:absolute;top:0; right:0; width:10px; height:28px; background:url(../images/layout/tabsItemLeft-sub-selected.gif) right bottom no-repeat #def1ff;}


.l-tab-content{ margin:0 auto; padding:0; border:none; width:100%;}
.l-tab-content-item{ width:100%; overflow:hidden; position:relative;}
.l-tab-content-item iframe{width:100%; border:none; height:100%;}


.l-tab-drag-proxy { border: 1px solid #BDD1EE;position: absolute;z-index: 1001;height:25px; line-height:25px; padding-left:19px; padding-right:4px; background:white;}
.l-tab-drag-proxy .l-drop-icon{position: absolute; top:5px; left:4px;width:16px; height:16px; }
.l-drop-no{background: white url('../images/common/drop-icons.gif') no-repeat 0px -20px; width:16px; height:16px; overflow:hidden;}
.l-drop-yes{background: white url('../images/common/drop-icons.gif') no-repeat 0px 0px;  width:16px; height:16px; overflow:hidden;}

.l-tab-drag-droptip{width:9px; position:absolute; top:30px; left:30px; height:44px; z-index:1000;}
.l-tab-drag-droptip .l-drop-move-up{ position:absolute; top:0px;}
.l-tab-drag-droptip .l-drop-move-down{ position:absolute; bottom:0px;}

.l-drop-move-up{background:transparent url('../images/common/drop-icons.gif') no-repeat -40px 0px; width:9px; height:9px; overflow:hidden;}
.l-drop-move-down{background: transparent url('../images/common/drop-icons.gif') no-repeat -60px 0px; width:9px; height:9px; overflow:hidden;}
/* -----------------------
   * easytab *
* ----------------------- */
.l-easytab{ padding:0px; position:relative;} 
.l-easytab ul{margin:0; padding:0; list-style:none;position:absolute; top:0px; left:0px; z-index:101;}
.l-easytab .l-easytab-header { padding-left:3px;}
.l-easytab .l-easytab-header li{ float:left; margin-left:2px;}
.l-easytab .l-easytab-header li span{ display:block; line-height:22px; height:22px; padding-left:8px; padding-right:8px; background:url('../images/controls/bg-tab-title.gif') repeat-x; color:#333; text-decoration:none;border:1px solid #AECAF0; color:#283B56;}
.l-easytab .l-easytab-header li span.l-over{background:url('../images/controls/bg-tab-title-over.gif') repeat-x;border:1px solid #DAB364; cursor:pointer;}
.l-easytab .l-easytab-header li span.l-selected{ background:none; background:white; border-top:3px solid #FFBD69;height:20px;color:#355686; border-left:1px solid  #DAB364;border-right:1px solid  #DAB364;border-bottom:1px solid  white; }
.l-easytab .l-easytab-panelbox{ border:1px solid #AECAF0; clear:left; position:absolute;top:23px; padding:4px;z-index:100;}


