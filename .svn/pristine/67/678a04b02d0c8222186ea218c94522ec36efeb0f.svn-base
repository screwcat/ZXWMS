					if(!globalUtil.isEmpty($(this).val())) {
						result = true;
						partflag = true;
						return false;
					}
				});
				$.each($(arr[i][j]).find('td:has(:checkbox)').children().get(), function(k, checkBox) {
					if($(checkBox).attr('checked') == 'checked'){
						result = true;
						partflag = true;
						return false;
					}
				});					
			}	
