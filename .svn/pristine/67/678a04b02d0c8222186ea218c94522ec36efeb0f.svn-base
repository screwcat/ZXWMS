						$(arr[i][j]).find('input:not(:checkbox,.hidden)').removeAttr('isRequired');
						$(arr[i][j]).find('select').removeAttr('isRequired');
						$(arr[i][j]).find('td:has(:checkbox)').removeAttr('checkboxIsRequired');
					}				
				}
			}
		});
