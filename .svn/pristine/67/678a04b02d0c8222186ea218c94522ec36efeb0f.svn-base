			arr[i].push(eleArr[parseInt(4 * i) + 3]);
			arr[i].push(eleArr[parseInt(4 * i) + 4]);
		}
		$.each(arr, function(i, ele) {
			partflag = false;
			for(j = 0;j < 4; j++){
				$(arr[i][j]).find('input:not(:checkbox,.hidden)').each(function(k) {
					if(!globalUtil.isEmpty($(this).val())){
						result = true;
