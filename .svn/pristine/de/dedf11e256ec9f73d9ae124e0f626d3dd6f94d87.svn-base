	//设置checkBox的Values
	function setCheckBoxValues(nameVal,valFromDb,ele,flag){
		if(flag){
			$(ele).find("input[name^='"+nameVal+"']").each(function(i) {
				var obj = $(this);
				for(var j = 0;j<valFromDb.length;j++){
					if(obj.val() == valFromDb[j]){
						obj.attr('checked', 'checked');
					}
				}
			});
			
		}else{
			$("input[name='"+nameVal+"']").each(function(i) {
				var obj = $(this);
				for(var j = 0;j<valFromDb.length;j++){
					if(obj.val() == valFromDb[j]){
						obj.attr('checked', 'checked');
					}
				}
			});
		}	
	}
	//设置Radio控件的值
	function setRadioVal(nameVal,valFromDb,ele,flag){
		if(flag){
			$(ele).find("input[name^='"+nameVal+"']").each(function() {
				if ($(this).val() == valFromDb) {
					$(this).attr('checked', 'checked');
					return false;
				}
			});
			
		}else{
			$("input[name='"+nameVal+"']").each(function() {
				if ($(this).val() == valFromDb) {
					$(this).attr('checked', 'checked');
					return false;
				}
			});
		}	
	}
