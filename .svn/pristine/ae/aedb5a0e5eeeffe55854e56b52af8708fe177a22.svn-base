<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="zh-CN" dir="ltr">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<title>合同打印</title>
<script language="Javascript" src="../../js/zx-all.js"> </script>

<script type="text/javascript">

	var wms_inve_transa_id=$.query.get('wms_inve_transa_id');
	var wms_inve_clerk_protocol_id = $.query.get("wms_inve_clerk_protocol_id");
	var wms_inve_clerk_protocol_transa_crepkg_id = $.query.get("wms_inve_clerk_protocol_transa_crepkg_id");

	var procotolIds=$.query.get('procotolIds');
	var url=$.query.get('url');
	
	$(function() {	
	    var requestUrl="/WMS/inve/"+url;
	    $('#form').attr("action",requestUrl);
	    $('#wms_inve_clerk_protocol_id').val(wms_inve_clerk_protocol_id);
	    $('#wms_inve_clerk_protocol_transa_crepkg_id').val(wms_inve_clerk_protocol_transa_crepkg_id);
	    $('#wms_inve_transa_id').val(wms_inve_transa_id);
	    $('#procotolIds').val(procotolIds);

    	$('#button1').click(function() {		
		 	$('#form').submit();
		}); 
		$('#button1').click(); 
    });
</script>
</head>
<body>
	<form method="post" id="form">
		<input type="hidden" name="wms_inve_transa_id" id="wms_inve_transa_id"/>
		<input type="hidden" name="wms_inve_clerk_protocol_id" id="wms_inve_clerk_protocol_id"/>
		<input type="hidden" name="wms_inve_clerk_protocol_transa_crepkg_id" id="wms_inve_clerk_protocol_transa_crepkg_id"/>
		<input type="hidden" name="procotolIds" id="procotolIds"/>
	</form>
	<input type="button" id="button1" value="加载中......"/>
</body>
</html>