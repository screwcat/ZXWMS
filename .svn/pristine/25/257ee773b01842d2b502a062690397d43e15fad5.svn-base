  function add_page(jsonForSys){
	  var comnum=jsonForSys.length;
	  if(comnum>7){
		  comnum=7;
	  }
	  var pagenum=20;//每页显示最小条数
		var max=22;//每页显示最大条数
		var number=1;//分页页码
		var sum=0;//当前页的条数
		var loop=30;
		for(var i=0;i<loop;i++){
			var num="#"+i; //通过id获取对象
			var val=parseInt($(num).attr("name")|0);
			if(i==1){
				sum=sum+val+comnum*1+13;//累加条数	
			}else{
				sum=sum+val;
			}
			if(sum>=pagenum){//如果累加条数大于最小条数
				var roll=0; //设置当前id的条数
				number=number+1;
				if(sum>=max){//如果累加条数大于最大条数
					i=i-1;
					num="#"+i;
					var val1=$(num);//获取达到最大条数的当前id的对象
					roll=parseInt($(num).attr("name")|0);//获取当前条数
				}else{
					var val1=$(num);	
				}
				//var sub=max-(sum-roll);//根据条数确定是否添加空行
				//setnbsp(sub,val1);
				var pageStr ='<p class=borrow_page align=center style=\'text-align:center;line-height:150%\'><b style=\'mso-bidi-font-weight:normal\'><span style=\'font-size:8.0pt;line-height:150%;mso-hansi-font-family:宋体\'>备注：非本债权人持本债权无效<span lang=EN-US><o:p></o:p></span></span></b></p>';
				pageStr += '</div>';
				pageStr += '<div class="page"></div>';
				pageStr += '<div  class="page2">';
				val1.append(pageStr);
				sum=0;
			}
			if(i==loop-2){
				var val2=$(num);
				var val1=$('#28');	
				var sub=max-sum;//根据条数确定是否添加空行
				setnbsp(sub,val2);
