		  var url = "";
		  if(judge == "bddy"){//如果judge不为空则为债权变动
			 url = "/inve/wmsinvedebtheaddyxysave.do";
		  }
		  else{
			  url = "/inve/wmsinvetransaprotocolsave.do";
		  }
	  $.post(globalUtil.getTimestampUrl(url),jsonStr, function(data) {   
		  if (data === 'success') 
		  {
			 globalUtil.successMsg(globalErrorMsg['100002'],function() 
					 {
							$("#dybtn").hide();
	 					   	$(".borrow_page").show();
	 						document.execCommand("print");
	 						   //$("#dybtn").show();
	 						$(".borrow_page").hide(); 
	 						if(flag == "agreementPrint") {
								window.opener.agreementPrint();
							}
							});//保存成功
						//刷新原有agreePrintList.html页面
						window.opener.search();
						
			}
		    else if (data === 'exist') 
		    {
			   globalUtil.errorMsg(globalErrorMsg['900127']); //名称重复！
			} 
		    else 
		    {
			   globalUtil.errorMsg(globalErrorMsg['100012']); //保存失败
			}
		});
	}
