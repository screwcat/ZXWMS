	<script type="text/javascript">
		var wms_cre_credit_head_id = $.query.get('wms_cre_credit_head_id');
		
		$(function() {
			//初始化数据
			init_data();
			//取消
			$('#cancel').click(function() {
				try {
					window.parent.dialog.hide();
				} catch (e) {
					globalUtil.closeCurrentTab();
				}
			});
		});

		function init_data(protocol_type_val) {
			var data = { "wms_cre_credit_head_id" : wms_cre_credit_head_id }
			$.ajax({
						type : "POST",
						url : global_param.context_name
								+ "/loanappro/getBorrowProtocolDetails.do",
						async : false,
						dataType : "json",
						data : data,
						success : function(data) {
							//单据
							$("#bill_code").val(data.bill_code);
							$("#protocol_id_year_num").val(data.protocol_id_year_num);
							$("#bill_code_group").val(data.bill_code_group);
							//债权人
							$("#creditor_name").val(data.creditor_name);
							//债务人
							$("#debtor_name").val(data.debtor_name);
							$("#debtor_identity_id").val(data.debtor_identity_id);
							$("#debtor_tel").val(data.debtor_tel);
							$("#gender").val(data.gender);
							$("#debtor_address").val(data.debtor_address);
							$("#com_debtor_name").val(data.com_debtor_name);
							$("#com_debtor_identity_id").val(data.com_debtor_identity_id);
							$("#com_debtor_tel").val(data.com_debtor_tel);
							$("#com_gender").val(data.com_gender);
							$("#com_debtor_address").val(data.com_debtor_address);
							//台账
							$("#cre_loan_type").val(data.cre_loan_type);
							$("#borrow_deadline").val(data.borrow_deadline);
							$("#borrow_interest").val(data.borrow_interest);
							$("#service_cost_month").val(data.service_cost_month);
							$("#consult_service_cost").val(data.consult_service_cost);
							$("#weiyuejin").val(data.weiyuejin);
							$("#appro_limit").val(data.appro_limit);
							$("#principal_lower").val(data.principal_lower);
							$("#refund_limit_month").val(data.refund_limit_month);
							$("#notarial_fee").val(data.platform_fee);
							$("#fees").val(data.fees);
							$("#liquidated_damages").val(data.liquidated_damages);
							$("#it_cost_fee").val(data.it_cost_fee);
							$("#expedited_fee").val(data.expedited_fee);
							$("#loan_amount").val(data.loan_amount);
							$("#borrow_begin_date").val(data.borrow_begin_date);
							$("#borrow_end_date").val(data.borrow_end_date);
							$("#first_refund_date").val(data.first_refund_date);
							$("#bill_type").val(data.bill_type);
							$("#the_number").val(data.the_number);
							$("#debtor_loan_name").val(data.debtor_loan_name);
							$("#debtor_loan_number").val(data.debtor_loan_number);
							$("#debtor_loan_bank").val(data.debtor_loan_bank);
							$("#refund_name").val(data.refund_name);
							$("#refund_number").val(data.refund_number);
							$("#refund_bank").val(data.refund_bank);
							$("#salesman_name_str").val(data.salesman_name_str);
							$("#team_manager").val(data.team_manager);
							$("#dept_name").val(data.dept_name);
							$("#depart").val(data.depart);
							$("#justice_end_time").val(data.justice_end_time);
							$("#justice_type").val(data.justice_type);
							//房产信息
							$("#explicit_address").val(data.explicit_address);
							$("#house_owner").val(data.house_owner);
							$("#house_area").val(data.house_area);
