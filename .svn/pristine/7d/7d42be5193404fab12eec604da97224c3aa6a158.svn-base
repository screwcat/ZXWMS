    var ismajchged=function(dom,val){
        var column, cellObj, rowdata;
        var ismaidold='',ismaidnew='';
        if(val=="1"){
            column = grid.columns[3];//获得主贷人列
            var gd=grid.getData();//获得grid数据
            for (var i = 0; i < gd.length; i++) {
