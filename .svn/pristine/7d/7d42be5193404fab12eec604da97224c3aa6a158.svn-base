      //实现查看单个表单信息的方法
    function setCreditVal(json){
        //------------------------贷款信息-----------------------------------
        //借款用途
        $('#credit_purpose').val(json.credit_purpose);
        //可接收每月最高还款额
        $('#max_repayment_limit_per_month').val(json.max_repayment_limit_per_month);
        //申请贷款额度
        $('#credit_limit').val(json.credit_limit);
        //申请最长还款期限
        $('#max_repayment_time_limit').val(json.max_repayment_time_limit);
        //执行利率
        $('#lending_rates_execution_cycles').val(json.lending_rates_execution_cycles);
        
        //业务员 判断如果业务员名称或者用户名编号存在 显示  否则不显示 
        if(json.salesman_name !='' || json.salesman_shortcode !=null){
            $('#salesman_name').val(json.salesman_name+"/"+json.salesman_shortcode);            
        }
        $('#salesman_id').val(json.salesman_id);
        $('#salesman_code').val(json.salesman_code);
        $('#city').val(json.city);
        $('#salesman_city_code').val(json.salesman_city_code);
        $('#salesman_dept_id').val(json.salesman_dept_id);
        //利率
        if(json.loan_interest_rate != null && json.loan_interest_rate != "") {
            $('#loan_interest_rate').val(parseFloat(json.loan_interest_rate).toFixed(2));
        }
        if(json.commission_rate != null && json.commission_rate != "") {
            $('#commission_rate').val(parseFloat(json.commission_rate).toFixed(2));
        }
        if(json.commission_fee != null && json.commission_fee != "") {
            $('#commission_fee').val(parseFloat(json.commission_fee).toFixed(2));
        }
        $('#is_xudai').val(json.is_xudai);
    }
    
    var updateSltRow;
    var passJSONStr=new Array();
    //修改用户信息
    var modify=function (){
        //选择某一行的操作
        var row = grid.getSelectedRow();
        //判断是否为空    
        if (row == null) {
                globalUtil.warnMsg(globalErrorMsg['100000']);//请选择一行记录进行修改
                return;
        }
        updateSltRow=row;
        //可以获取改行的所有字段属性alert(row.status);//0代表暂存  1代表提交
        if(row.status==0 || row.status==1){
            var url ='';
            if(row.wms_cre_credit_line_customer_change_head_id!=undefined&&row.wms_cre_credit_line_customer_change_head_id!=null){
                url = globalUtil.getHtml("modifyCreCustormForUpdateCredit.html?wms_cre_credit_line_customer_change_head_id="+row.wms_cre_credit_line_customer_change_head_id+"&isma="+row.is_major);
            }else if (row.wms_cus_customer_id!=undefined&&row.wms_cus_customer_id!=null) {
                url = globalUtil.getHtml("modifyCreCustormForUpdateCredit.html?wms_cus_customer_id="+row.wms_cus_customer_id+"&isma="+row.is_major);
            }
            var len = modifyJsonCus.length;
            for(var i =len-1; i>=0; i--){
                var data = modifyJsonCus[i];
                if(row.wms_cre_credit_line_customer_change_head_id!=undefined&&row.wms_cre_credit_line_customer_change_head_id!=null){
                    if(row.wms_cre_credit_line_customer_change_head_id == data.wms_cre_credit_line_customer_change_head_id){
                        passJSONStr=data;
                        url = globalUtil.getHtml("modifyCreCustormForUpdateCredit.html?datajson=1&isma="+row.is_major);
                        break;
                    }
